{
  "kind": "collectionType",
  "collectionName": "formularios",
  "info": {
    "singularName": "formulario",
    "pluralName": "formularios",
    "displayName": "Formulario",
    "description": "Formularios descargables para tr√°mites"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nombre": {
      "type": "string",
      "maxLength": 200,
      "required": true
    },
    "descripcion": {
      "type": "text",
      "required": true
    },
    "formato": {
      "type": "enumeration",
      "required": true,
      "default": "PDF",
      "enum": [
        "PDF",
        "Excel",
        "Word"
      ]
    },
    "tamano": {
      "type": "string",
      "required": false
    },
    "archivo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "categoria_formulario": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::categoria-formulario.categoria-formulario",
      "inversedBy": "formularios"
    },
    "orden": {
      "type": "integer",
      "default": 0
    },
    "activo": {
      "type": "boolean",
      "default": true
    },
    "requisitos": {
      "type": "richtext"
    },
    "instrucciones": {
      "type": "richtext"
    },
    "descargas": {
      "type": "integer",
      "default": 0
    }
  }
}
