    "descripcion": {
      "type": "blocks"
    },
    "resumen": {
      "type": "text"
    },
    "imagen": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "requisitos": {
      "type": "text"
    },
    "orden": {
      "type": "integer"
    },
    "activo": {
      "type": "boolean",
      "default": true
    },
    "imagenDetalle": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "destacado": {
      "type": "boolean",
      "default": false
    }
